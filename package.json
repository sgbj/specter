{
  "name": "specter",
  "productName": "specter",
  "version": "1.0.0",
  "description": "Screen recording app built with Electron",
  "main": "main.js",
  "scripts": {
    "electron": "electron",
    "electron-packager": "electron-packager",
    "gulp": "gulp",
    "start": "npm run electron .",
    "build": "npm run electron-packager -- . --all --out=dist",
    "postinstall": "npm run gulp -- --gulpfile semantic/gulpfile.js build"
  },
  "repository": "https://github.com/sgbj/specter",
  "keywords": [
    "screen",
    "recording",
    "Electron"
  ],
  "author": "sgbj",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.4.1",
    "electron-packager": "^8.1.0"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "semantic-ui": "^2.2.4"
  }
}
